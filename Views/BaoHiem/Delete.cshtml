        <dt>
            <th>Số máy</th>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SOMAY)
        </dd>
        <dt>
            <th>Thời gian bảo hiểm</th>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.THOIHANBD.Date)
        </dd>

        <dt>
            <th>Thời gian kết thúc</th>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.THOIHANKT.Date)
        </dd>

        <dt>
            <th>Phí bảo hiểm</th>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PHIBH)
        </dd>

        <dt>
            <th>Email người dùng</th>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Gmail)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Back to List", "ListBaoHiem")
        </div>
    }
</div>
